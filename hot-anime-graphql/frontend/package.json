{
  "name": "cra-template-redux-typescript",
  "version": "1.0.2",
  "scripts": {
    "format": "prettier --write ./src/**/*.{ts,tsx,css}",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "type-check": "tsc",
    "schema:download": "apollo service:download --endpoint=https://anilist.co/graphql ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"
  },
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux"
  ],
  "description": "The official Redux+TS template for Create React App",
  "main": "template.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/reduxjs/cra-template-redux-typescript.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.10"
  },
  "bugs": {
    "url": "https://github.com/reduxjs/cra-template-redux-typescript/issues"
  },
  "files": [
    "template",
    "template.json"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/redux-logger": "^3.0.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.1.1",
    "typescript": "~4.4.3"
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@apollo/graphql": "^14.5.0",
    "@types/styled-components": "^5.1.14",
    "graphql": "^15.6.0",
    "jest": "26.6.0",
    "redux-logger": "^3.0.6",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
